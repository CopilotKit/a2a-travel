{
  "name": "ag-ui-adk-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"UI,Orchestrator,Itinerary,Budget\" --prefix-colors \"cyan,gray,magenta,blue\" \"npm run dev:ui\" \"npm run dev:orchestrator\" \"npm run dev:itinerary\" \"npm run dev:budget\"",
    "dev:ui": "next dev --turbopack",
    "dev:orchestrator": "agents/.venv/bin/python agents/orchestrator.py",
    "dev:itinerary": "agents/.venv/bin/python agents/itinerary_agent.py",
    "dev:budget": "agents/.venv/bin/python agents/budget_agent.py",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ag-ui/a2a-middleware": "^0.0.1",
    "@ag-ui/client": "0.0.40",
    "@a2a-js/sdk": "latest",
    "@copilotkit/react-core": "latest",
    "@copilotkit/react-ui": "latest",
    "@copilotkit/runtime": "latest",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^8",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "overrides": {
    "@ag-ui/client": "0.0.40"
  }
}
