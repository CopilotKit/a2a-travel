# Google API Key (for ADK agents: Orchestrator and Budget Agent)
# Get your key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (for LangGraph Itinerary Agent)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Agent URLs (optional - defaults shown below)
ORCHESTRATOR_URL=http://localhost:9000
ITINERARY_AGENT_URL=http://localhost:9001
BUDGET_AGENT_URL=http://localhost:9002

# Agent Ports (optional - for Python agents)
ORCHESTRATOR_PORT=9000
ITINERARY_PORT=9001
BUDGET_PORT=9002
